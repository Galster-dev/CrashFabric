{
  "schemaVersion": 1,
  "id": "crashfabric",
  "version": "${version}",
  "name": "CrashFabric",
  "description": "Attempts to fix most of the server crashes (based on PaperMC project)",
  "authors": [
    "Galster"
  ],
  "contact": {
    "repo": "https://github.com/Galster-dev/CrashFabric.git"
  },
  "license": "LGPL-3.0",
  "icon": "assets/crashfabric/icon.png",
  "environment": "server",
  "entrypoints": {
    "main": [
      "ru.galster.crashfabric.ServerModInitializer"
    ]
  },
  "mixins": [
    "crashfabric.mixins.json"
  ],
  "accessWidener": "crashfabric.accesswidener",
  "depends": {
    "fabricloader": ">=0.14.9",
    "minecraft": ">=1.19"
  }
}
